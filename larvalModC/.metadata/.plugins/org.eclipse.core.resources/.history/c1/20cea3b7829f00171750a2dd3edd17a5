// function example
#include <iostream>
#include <vector>
using namespace std;

int mod(double uoE, double uoL, int time) {
	int t;
	t = 0;
    int r;

	while (t < time) {

		uoE = uoE + (uoE * uoL);
		t++;
		r=uoE;

	}

	return r;

}

int main ()
{
  int z;
  z =  mod(10,0.2,1);
  cout << "The result is " << z;
}
